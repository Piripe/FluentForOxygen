<images>
    <image width="12" height="6" name="t.tga">
        <ellipse x="0" y="0" width="12" height="12" fill="Background_FillColor_Acrylic_Default"/>
            <src if="enableAcrylic" color="Color.FromArgb(3,255,255,255)">rounded_noise.png</src>
    </image>
    <image width="12" height="6" name="b.tga">
        <ellipse x="0" y="-6" width="12" height="12" fill="Background_FillColor_Acrylic_Default"/>
            <src if="enableAcrylic" offset="0 -6" color="Color.FromArgb(3,255,255,255)">rounded_noise.png</src>
    </image>
    <folder name="handle">
        <image width="6" height="3" name="t.tga">
            <ellipse x="0" y="0" width="6" height="6" fill="StrokeColor_ControlStrongStroke_Default"/>
        </image>
        <image width="6" height="3" name="b.tga">
            <ellipse x="0" y="-3" width="6" height="6" fill="StrokeColor_ControlStrongStroke_Default"/>
        </image>
    </folder>
    <folder name="arrows">
        <image width="8" height="6" name="up.tga">
            <src color="StrokeColor_ControlStrongStroke_Default">arrow_up.png</src>
        </image>
        <image width="8" height="6" name="upHover.tga">
            <src color="StrokeColor_ControlStrongStroke_Secondary">arrow_up.png</src>
        </image>
        <image width="8" height="6" name="down.tga">
            <src color="StrokeColor_ControlStrongStroke_Default">arrow_down.png</src>
        </image>
        <image width="8" height="6" name="downHover.tga">
            <src color="StrokeColor_ControlStrongStroke_Secondary">arrow_down.png</src>
        </image>
    </folder>
</images>