<images>
    <folder name="albumImage">
        <folder name="background">
            <image width="4" height="4" name="bl.tga">
                <rect x="-4" y="0" width="8" height="8" border-radius="-4" fill="Background_FillColor_SolidBackground_Base"/>
                <rect x="-4" y="0" width="8" height="8" border-radius="-4" fill="Background_FillColor_CardBackground_Secondary"/>
            </image>
            <image width="4" height="4" name="br.tga">
                <rect x="0" y="0" width="8" height="8" border-radius="-4" fill="Background_FillColor_SolidBackground_Base"/>
                <rect x="0" y="0" width="8" height="8" border-radius="-4" fill="Background_FillColor_CardBackground_Secondary"/>
            </image>
            <image width="4" height="4" name="tl.tga">
                <rect x="-4" y="-4" width="8" height="8" border-radius="-4" fill="Background_FillColor_SolidBackground_Base"/>
                <rect x="-4" y="-4" width="8" height="8" border-radius="-4" fill="Background_FillColor_CardBackground_Secondary"/>
            </image>
            <image width="4" height="4" name="tr.tga">
                <rect x="0" y="-4" width="8" height="8" border-radius="-4" fill="Background_FillColor_SolidBackground_Base"/>
                <rect x="0" y="-4" width="8" height="8" border-radius="-4" fill="Background_FillColor_CardBackground_Secondary"/>
            </image>
        </folder>
    </folder>
    <folder name="buttons">
        <folder name="browse">
            <image width="16" height="16" name="normal.tga">
                <src color="FillColor_Text_Primary">browse.png</src>
            </image>
        </folder>
        <folder name="mute">
            <image width="16" height="16" name="normal.tga">
                <src color="FillColor_Text_Primary">mute.png</src>
            </image>
        </folder>
        <folder name="next">
            <image width="16" height="16" name="normal.tga">
                <src color="FillColor_Text_Primary">next.png</src>
            </image>
            <image width="16" height="16" name="hover.tga">
                <src color="AccentColor_Secondary">next.png</src>
            </image>
        </folder>
        <folder name="pause">
            <image width="50" height="50" name="normal.tga">
                <src color="FillColor_Text_Primary">pause.png</src>
            </image>
            <image width="50" height="50" name="hover.tga">
                <src color="AccentColor_Secondary">pause.png</src>
                <ellipse x="0" y="0" width="50" height="50" stroke="AccentColor_Secondary" stroke-width="3" stroke-align="inside" />
            </image>
            <image width="50" height="50" name="hover.tga">
                <src color="FillColor_Text_Primary">pause.png</src>
                <ellipse x="0" y="0" width="50" height="50" stroke="AccentColor_Secondary" stroke-width="3" stroke-align="inside" />
            </image>
        </folder>
        <folder name="play">
            <image width="50" height="50" name="normal.tga">
                <src color="FillColor_Text_Primary">play.png</src>
            </image>
            <image width="50" height="50" name="hover.tga">
                <src color="AccentColor_Secondary">play.png</src>
                <ellipse x="0" y="0" width="50" height="50" stroke="AccentColor_Secondary" stroke-width="3" stroke-align="inside" />
            </image>
            <image width="50" height="50" name="hover.tga">
                <src color="FillColor_Text_Primary">play.png</src>
                <ellipse x="0" y="0" width="50" height="50" stroke="AccentColor_Secondary" stroke-width="3" stroke-align="inside" />
            </image>
        </folder>
        <folder name="previous">
            <image width="16" height="16" name="normal.tga">
                <src color="FillColor_Text_Primary">previous.png</src>
            </image>
            <image width="16" height="16" name="hover.tga">
                <src color="AccentColor_Secondary">previous.png</src>
            </image>
        </folder>
        <folder name="repeat">
            <image width="16" height="16" name="normal.tga">
                <src color="FillColor_Text_Primary">repeat_no.png</src>
            </image>
            <image width="16" height="16" name="selected.tga">
                <src color="FillColor_TextOnAccent_Primary">repeat_yes.png</src>
            </image>
        </folder>
        <folder name="repeatOnce">
            <image width="16" height="16" name="selected.tga">
                <src color="FillColor_TextOnAccent_Primary">repeat_once.png</src>
            </image>
        </folder>
        <folder name="shuffle">
            <image width="16" height="16" name="normal.tga">
                <src color="FillColor_Text_Primary">shuffle_no.png</src>
            </image>
            <image width="16" height="16" name="selected.tga">
                <src color="FillColor_TextOnAccent_Primary">shuffle_yes.png</src>
            </image>
        </folder>
        <folder name="volume">
            <image width="16" height="16" name="normal.tga">
                <src color="FillColor_Text_Primary">volume.png</src>
            </image>
        </folder>
    </folder>
</images>